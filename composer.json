{
    "name": "azine/hybridauth-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to access the profiles via Xing- and Linkedin-API, uses HybridAuth",
    "keywords": ["xing", "oauth", "xing api"],
    "homepage": "https://github.com/azine/AzineHybridAuthBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Azine IT Services AG",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1,>=2.1",
        "symfony/console": "~2.1,>=2.1",
        "symfony/finder": "~2.1,>=2.1",
        "symfony/twig-bundle": "~2.1,>=2.1",
	    "hybridauth/hybridauth": "~2.3.0",
        "monolog/monolog": "~1.6,>=1.6.0",
        "twig/extensions": "~1.0"
    },
    "autoload": {
        "psr-0": { "Azine\\HybridAuthBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Azine/HybridAuthBundle"
}
